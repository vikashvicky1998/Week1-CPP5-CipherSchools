#include<bits/stdc++.h>

using namespace std;

int sum(int n1, int n2){
    return n1+n2;
}

int main() {
    int n1,n2;
    cin>n1>n2;

    cout<<"the sum of n1 & n2 is : "<<sum(n1,n2);  

	return 0;
}



